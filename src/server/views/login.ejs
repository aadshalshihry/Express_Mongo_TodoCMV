<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1><%= title %></h1>
    <p>Welcome to <%= title %></p>

    <br>
    <% for (var i = 0; i < todos.length; i++){ %>
        <h1><%= todos[i] %></h1>
    <% } %>
    <br>
    <% todos.forEach(function(todo){ %>
        <h1><%= todo.name %></h1>
    <% }) %>
  </body>
</html>
